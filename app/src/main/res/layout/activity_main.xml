<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/dl_activity_main"
    tools:context=".MainActivity">
    <FrameLayout
        android:id="@+id/fl_activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="high.co.kr.project.HomeFragment">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical"
            android:weightSum="1"
            android:background="#FFFFFF">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:weightSum="0.1">

                <TextView
                    android:id="@+id/gps_address"
                    android:paddingLeft="10dp"
                    android:drawableLeft="@drawable/gps"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="address"
                    android:background="@drawable/home_btnclass"
                    android:layout_gravity="center"
                    />

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:weightSum="1"
                android:background="#FFFFFF">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2">

                    <ImageButton
                        android:id="@+id/Tbtn1"
                        android:layout_width="wrap_content"
                        android:layout_height="100dip"
                        android:layout_weight="50"
                        android:onClick="mOnClick1"
                        android:src="@drawable/title"
                        android:textColor="#ff6666"
                        android:textSize="50dp"
                        android:text="Title"
                        android:background="@color/white"
                        />

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2">
                    <ImageButton
                        android:id="@+id/cafe_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="cafeBtn"
                        android:src="@drawable/cafe"
                        android:background="@drawable/home_btnclass"/>
                    <ImageButton
                        android:id="@+id/fast_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="fastBtn"
                        android:src="@drawable/fastfood"
                        android:background="@drawable/home_btnclass"/>

                    <ImageButton
                        android:id="@+id/meat_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="meatBtn"
                        android:src="@drawable/meat"
                        android:background="@drawable/home_btnclass"/>
                </LinearLayout>


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2">
                    <ImageButton
                        android:id="@+id/pizz_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="pizzBtn"
                        android:src="@drawable/pizza"
                        android:background="@drawable/home_btnclass"
                        />
                    <ImageButton
                        android:id="@+id/pasta_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="pastaBtn"
                        android:src="@drawable/pasta"
                        android:background="@drawable/home_btnclass"
                        />

                    <ImageButton
                        android:id="@+id/chk_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="chkBtn"
                        android:src="@drawable/chieken"
                        android:background="@drawable/home_btnclass"/>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2">
                    <ImageButton
                        android:id="@+id/kr_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="krBtn"
                        android:src="@drawable/korea"
                        android:background="@drawable/home_btnclass"
                        />
                    <ImageButton
                        android:id="@+id/cn_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:onClick="cnBtn"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/china"
                        android:background="@drawable/home_btnclass"
                        />

                    <ImageButton
                        android:id="@+id/jp_Btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="jpBtn"
                        android:src="@drawable/japan"
                        android:background="@drawable/home_btnclass"/>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2">
                    <ImageButton
                        android:id="@+id/category_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="98dip"
                        android:layout_weight="33"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="cartBtn"
                        android:src="@drawable/cart_img"
                        android:background="@drawable/home_btnclass"

                        />
                    <ImageButton
                        android:id="@+id/map_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="98dip"
                        android:layout_weight="33"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="fav_OnClick"
                        android:src="@drawable/favorite_img"
                        android:background="@drawable/home_btnclass"/>

                    <ImageButton
                        android:id="@+id/orderlist_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="98dip"
                        android:layout_weight="33"
                        android:adjustViewBounds="true"
                        android:scaleType="fitXY"
                        android:onClick="order_OnClick"
                        android:src="@drawable/list_img"
                        android:background="@drawable/home_btnclass"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </FrameLayout>

    <LinearLayout
        android:id="@+id/lv_activity_main"
        android:background="#EDEDED"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_gravity="start" >
        <TextView
            android:id="@+id/lv_activity_mileage"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:paddingTop="20dp"
            android:textSize="10pt"
            android:textColor="#2C3E50"/>
        <ListView android:id="@+id/lv_activity_list"
            android:background="#EDEDED"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:choiceMode="singleChoice"
            android:divider="@android:color/transparent"
            android:dividerHeight="0dp">
        </ListView>
    </LinearLayout>
</android.support.v4.widget.DrawerLayout>