<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="100dip"
    android:padding="10dp"
    android:background="@color/white"
    android:orientation="horizontal">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/white">
        <TextView
            android:id="@+id/f_s_name"
            android:layout_marginTop="15dp"
            android:layout_width="wrap_content"
            android:layout_height="40dip"
            android:textStyle="bold"
            android:text="StoreName"
            android:textSize="25dip"
            android:textColor="@color/darkblue"/>
        <TextView
            android:id="@+id/f_s_tel_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_below="@+id/f_s_name"
            android:textSize="15dip"
            android:text="Tel : "
            android:textColor="@color/darkblue"/>
        <TextView
            android:id="@+id/f_s_tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/f_s_tel_t"
            android:layout_below="@+id/f_s_name"
            android:text="02-000-0000"
            android:textSize="13dip"/>
        <TextView
            android:id="@+id/f_s_addr_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="13dip"
            android:text="Addr : "
            android:textColor="@color/darkblue"
            android:layout_marginLeft="160dp"
            />
        <TextView
            android:id="@+id/f_s_addr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dip"
            android:layout_toRightOf="@+id/f_s_addr_t"
            android:text="서울시 정릉동 서경대학교 북악관 602호"
            android:maxLines="2"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/f_s_time_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="13dip"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="160dp"
            android:text="영업 시간 : "
            android:textColor="@color/darkblue"
            android:layout_below="@+id/f_s_addr_t"
            />
        <TextView
            android:id="@+id/f_s_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dip"
            android:text="10~20"
            android:maxLines="2"
            android:ellipsize="end"
            android:layout_below="@+id/f_s_addr"
            android:layout_toRightOf="@+id/f_s_count_t"
            android:layout_toEndOf="@+id/f_s_count_t" />
        <TextView
            android:id="@+id/f_s_count_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="13dip"
            android:text="방문 횟수 : "
            android:textColor="@color/darkblue"
            android:layout_below="@+id/f_s_time"
            android:layout_alignLeft="@+id/f_s_time_t"
            android:layout_alignStart="@+id/f_s_time_t" />
        <TextView
            android:id="@+id/f_s_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dip"
            android:text="10번"
            android:maxLines="2"
            android:ellipsize="end"
            android:layout_below="@+id/f_s_time_t"
            android:layout_toRightOf="@+id/f_s_time_t"
            android:layout_toEndOf="@+id/f_s_time_t" />

        <Button
            android:id="@+id/btn_del"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:layout_toRightOf="@+id/f_s_time"
            android:layout_below="@+id/f_s_addr"
            android:layout_marginLeft="10dip"
            android:text="삭제"
            android:textColor="@color/white"/>

    </RelativeLayout>


</LinearLayout>